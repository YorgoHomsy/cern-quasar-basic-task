<?xml version="1.0" encoding="UTF-8"?>
<d:design projectShortName="OpcUaSampleServerNameChangeItInDesign" xmlns:d="http://cern.ch/quasar/Design" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://cern.ch/quasar/Design Design.xsd ">
  <d:class name="Person">
  	<d:devicelogic></d:devicelogic>
  	<d:configentry dataType="UaString" name="Name"></d:configentry>
  	<d:cachevariable initializeWith="valueAndStatus"
  		dataType="OpcUa_Double" name="HeightSet" nullPolicy="nullForbidden"
  		addressSpaceWrite="regular" initialStatus="OpcUa_Good"
  		initialValue="0">
  	</d:cachevariable>
  	<d:cachevariable initializeWith="valueAndStatus"
  		dataType="OpcUa_Double" name="ActualHeight" nullPolicy="nullAllowed"
  		addressSpaceWrite="regular" initialStatus="OpcUa_Good"
  		initialValue="0">
  	</d:cachevariable>
  	<d:cachevariable initializeWith="valueAndStatus"
  		dataType="OpcUa_Double" name="WeightSet" nullPolicy="nullForbidden"
  		addressSpaceWrite="regular" initialStatus="OpcUa_Good"
  		initialValue="0">
  	</d:cachevariable>
  	<d:cachevariable initializeWith="valueAndStatus"
  		dataType="OpcUa_Double" name="ActualWeight" nullPolicy="nullAllowed"
  		addressSpaceWrite="regular" initialStatus="OpcUa_Good"
  		initialValue="0">
  	</d:cachevariable>
  	<d:method addressSpaceCallUseMutex="no" name="eat"
  		executionSynchronicity="synchronous">
  		<d:argument dataType="OpcUa_Double" name="amount"></d:argument>
  	</d:method>
  </d:class>
  <d:class name="Family">
  	<d:devicelogic></d:devicelogic>
  	<d:hasobjects instantiateUsing="configuration" class="Person"></d:hasobjects>
  	<d:configentry dataType="UaString" name="Name"></d:configentry>
  	<d:configentry dataType="UaString" name="City"></d:configentry>
  </d:class>
  <d:root>
  	<d:hasobjects instantiateUsing="configuration" class="Family"></d:hasobjects></d:root>
</d:design>